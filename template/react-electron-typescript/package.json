{
  "name": "react-electron-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "start-web": "cross-env BROWSER=none react-scripts start",
    "start-app": "ts-node --skip-project app/connect-app.ts",
    "watch-app": "tsc --watch --project app/tsconfig.json",
    "prepare": "node .githooks/config.js",
    "commit": "node .githooks/customize.js",
    "release": "standard-version --no-verify"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "cross-env": "^7.0.3",
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "foreman": "^3.0.1",
    "inquirer": "^8.1.2",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.2.1"
  }
}
